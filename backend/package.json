{
  "name": "expenses-tracker-backend",
  "version": "1.0.0",
  "description": "Backend for an personal expenses tracker app",
  "main": "server.js",
  "scripts": {
    "dev": "npx sequelize db:migrate && npx nodemon --exec babel-node server.js",
    "dev:debug": "npx sequelize db:migrate && npx nodemon --exec babel-node --inspect server.js",
    "seed:all": "npx sequelize db:seed:all",
    "seed:undo": "npx sequelize db:seed:undo",
    "lint": "npx eslint --fix src --ext .js"
  },
  "keywords": [
    "node",
    "js",
    "express",
    "jwt",
    "authentication",
    "postgresql"
  ],
  "author": "legermano",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.19.0",
    "sequelize-cli": "^6.4.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.17.10",
    "@babel/node": "^7.17.10",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/preset-env": "^7.17.10",
    "eslint": "^8.15.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2"
  }
}
